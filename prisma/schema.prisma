generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model administrative_divisions {
  id                             Int                        @id @default(autoincrement())
  code                           String                     @db.VarChar(20)
  name                           String                     @db.VarChar(100)
  country_id                     Int?
  country_code                   String                     @db.Char(2)
  parent_id                      Int?
  level                          Int?                       @db.UnsignedTinyInt
  created_at                     DateTime                   @default(now()) @db.Timestamp(0)
  updated_at                     DateTime?                  @db.Timestamp(0)
  countries                      countries?                 @relation(fields: [country_id], references: [id], onDelete: Restrict, map: "administrative_divisions_country_id_foreign")
  administrative_divisions       administrative_divisions?  @relation("administrative_divisionsToadministrative_divisions_parent_id", fields: [parent_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "administrative_divisions_parent_id_foreign")
  other_administrative_divisions administrative_divisions[] @relation("administrative_divisionsToadministrative_divisions_parent_id")

  @@unique([country_id, level, code], map: "administrative_divisions_code_unique")
  @@index([parent_id], map: "administrative_divisions_parent_id_foreign")
}

model ambients {
  id             Int              @id @default(autoincrement())
  code           String           @db.VarChar(255)
  name           String?          @db.VarChar(255)
  title          String?          @db.VarChar(255)
  subtitle       String?          @db.VarChar(255)
  description    String?          @db.VarChar(255)
  background     String           @db.Text
  keywords       String?          @db.Text
  active         Boolean          @default(true)
  sponsor_id     Int?
  created_at     DateTime?        @db.Timestamp(0)
  updated_at     DateTime?        @db.Timestamp(0)
  ambients_items ambients_items[]
}

model ambients_items {
  id                Int                 @id @default(autoincrement())
  ambient_id        Int
  type              String              @db.VarChar(255)
  x                 Float
  y                 Float
  code              String?             @db.VarChar(255)
  reference         String?             @db.VarChar(255)
  name              String?             @db.VarChar(255)
  description       String?             @db.VarChar(255)
  image             String              @db.Text
  query             String?             @db.VarChar(255)
  filters           String?             @db.VarChar(255)
  active            Boolean             @default(true)
  created_at        DateTime?           @db.Timestamp(0)
  updated_at        DateTime?           @db.Timestamp(0)
  x_mobile          Float
  y_mobile          Float
  image_mobile      String              @db.Text
  ambients          ambients            @relation(fields: [ambient_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "ambients_items_ambient_id_foreign")
  ambients_subitems ambients_subitems[]

  @@index([ambient_id], map: "ambients_items_ambient_id_foreign")
}

model ambients_subitems {
  id              Int             @id @default(autoincrement())
  ambient_item_id Int?
  code            String?         @db.VarChar(255)
  reference       String?         @db.VarChar(255)
  name            String?         @db.VarChar(255)
  description     String?         @db.VarChar(255)
  query           String?         @db.VarChar(255)
  filters         String?         @db.VarChar(255)
  active          Boolean         @default(true)
  created_at      DateTime?       @db.Timestamp(0)
  updated_at      DateTime?       @db.Timestamp(0)
  parent          Int?
  ambients_items  ambients_items? @relation(fields: [ambient_item_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "ambients_subitems_ambient_item_id_foreign")

  @@index([ambient_item_id], map: "ambients_subitems_ambient_item_id_foreign")
}

model applications {
